{
  "manifest_version": 3,
  "name": "cookie.jar",
  "version": "1.0.0",
  "description": "A modern cookie manager to view, edit, add, delete, and import/export browser cookies.",
  "action": {
    "default_popup": "index.html",
    "default_title": "cookie.jar"
  },
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "options_page": "options.html",
  "homepage_url": "https://github.com/SparkiePG/cookiejar"
}